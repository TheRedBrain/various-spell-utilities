{
	"schemaVersion": 1,
	"id": "variousspellutilities",
	"version": "${version}",
	"name": "Various Spell Utilities",
	"description": "Small utility mod that adds various supporting content, which can be used by data packs and content mods.",
	"authors": [
		"TheRedBrain"
	],
	"contact": {
		"sources": "https://github.com/TheRedBrain/various-spells"
	},
	"license": "MIT",
	"icon": "assets/variousspellutilities/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.github.theredbrain.variousspells.VariousSpellUtilities"
		],
		"client": [
			"com.github.theredbrain.variousspells.VariousSpellUtilitiesClient"
		]
	},
	"mixins": [
		"variousspellutilities.mixins.json",
		{
			"config": "variousspellutilities.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"minecraft": "${minecraft_version}",
		"fabricloader": ">=${loader_version}",
		"fabric-api": "*",
		"cloth-config": ">=${cloth_config_version}",
		"spell_engine": ">=${spell_engine_version}",
		"ranged_weapon_api": ">=${ranged_weapon_api_version}"
	}
}